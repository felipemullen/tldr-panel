{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"lib": [
			"ES2020"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"skipLibCheck": true,
		"noImplicitReturns": true,
		"noUnusedParameters": true
	},
	"exclude": [
		"__mocks__/**/*.*",
		"__tests__/**/*.*"
	],
	"include": [
		"**/*.ts",
	]
}